<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SERVReserva</servlet-name>
        <servlet-class>controlador.SERVReserva</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SERVPersona</servlet-name>
        <servlet-class>controlador.SERVPersona</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SERVCinefilo</servlet-name>
        <servlet-class>controlador.SERVCinefilo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SERVLogeo</servlet-name>
        <servlet-class>controlador.SERVLogeo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SERVConsultar</servlet-name>
        <servlet-class>controlador.SERVConsultar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SERVReserva</servlet-name>
        <url-pattern>/SERVReserva</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SERVPersona</servlet-name>
        <url-pattern>/SERVPersona</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SERVCinefilo</servlet-name>
        <url-pattern>/SERVCinefilo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SERVLogeo</servlet-name>
        <url-pattern>/SERVLogeo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SERVConsultar</servlet-name>
        <url-pattern>/SERVConsultar</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
